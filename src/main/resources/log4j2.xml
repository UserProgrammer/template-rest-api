<?xml version="1.0" encoding="UTF-8"?>

<configuration>
  <appenders>
    <console name="consle" target="SYSTEM_OUT">
      <patternLayout pattern="%d{HH:mm:ss.SSS [%t] %-5level %logger{36} - %msg%n"/>
    </console>
  </appenders>

  <loggers>
    <root level="warn">
      <appender-ref ref="console" />
    </root>
  </loggers>
</configuration>
